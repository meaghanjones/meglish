<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %> | Meglish</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Abril+Fatface|Open+Sans" rel="stylesheet">

  </head>

  <body>

    <div class="collapse navbar-collapse" id="navbar-collapse-1">
           <ul class="nav navbar-nav navbar-right">
              <li><%= link_to 'Course List', courses_path %></li>
               <% if current_user %>
               <li><%= link_to 'Edit Profile',edit_user_registration_path %></li>
               <li><%= link_to 'Logout', destroy_user_session_path, method: :delete %></li>
               <% end %>
          </ul>
    </div>
<div class="wrapper">
    <%= yield %>

    <%= flash[:alert] %>
    <%= flash[:notice] %>

  <footer class ="footer-basic-centered">
    <p class="footer-company-motto" >Learn English with Meglish</p>
    <p class="footer-links"><%= link_to 'Course List', courses_path %>&nbsp;&nbsp;&#0149;&nbsp;&nbsp;<%= link_to 'About', static_pages_about_path %>&nbsp;&nbsp;&#0149;&nbsp;&nbsp;<a href="#">Home</a>&nbsp;&nbsp;&#0149;&nbsp;&nbsp;<a href="#">Home</a></p>
  </footer>
</div>
  </body>
</html>
